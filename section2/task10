class Car:
def __init__(self, brand, year, mileage=0):
self.brand = brand
self.year = year
self.mileage = mileage

def mileage(self):
return self.__mileage

def mileage(self, value):
if value < 0:
print("пробіг не може бути від’ємним")
else:
self.__mileage = value

def drive(self, km):
if km > 0:
self.mileage += km

def __str__(self):
return f"{self.brand} ({self.year}), пробіг: {self.mileage} км"

car = Car("Toyota", 2019, 50000)
car.drive(150)

car.mileage = -100

print(car)
