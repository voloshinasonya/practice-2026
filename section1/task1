import this

a = 12
b = 5

x = 3.5
y = 2.0

s1 = "Hello"
s2 = "Python"

print("a + b =", a + b)
print("a - b =", a - b)
print("a * b =", a * b)
print("a / b =", a / b)

print("x + y =", x + y)
print("x * y =", x * y)

print("Результат конкатенації:", s1 + " " + s2)

num = int(input("Введіть ціле число: "))
print("Ви ввели число:", num)
